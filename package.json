{
  "private": true,
  "name": "htmx-dnd",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon --watch public/src --watch server --ext html,js --exec \"npm run build && netlify dev --no-open\"",
    "build": "npm run build.style",
    "build.style": "tailwindcss -i ./server/app/style.css -o ./public/style.css",
    "test": "nodemon --watch public/src --ext js public/src/actions.test.js",
    "test.once": "node public/src/actions.test.js"
  },
  "devDependencies": {
    "netlify-cli": "16.3.3",
    "nodemon": "3.0.1",
    "postcss-import": "15.1.0",
    "tailwindcss": "3.3.3"
  },
  "dependencies": {
    "ejs": "3.1.9",
    "mongoose": "7.5.3",
    "pathern": "0.3.1"
  }
}
